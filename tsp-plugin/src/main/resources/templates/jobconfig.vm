<!--菜单右键新增周期翻牌-->
<div class="modal-modify modal-turnover" style="display: none">
    <div class="modal-body">
        <form class="form-horizontal" id="form_turnover" action="#pluginLink('tsp/jobconfig/addnewjobconfig')" method="post">
            <input type="text" class="hide" value="" id="root-turnover" name="root"/>
            <input type="text" class="hide" value="" id="type-turnover" name="jobType"/>
            <input type="text" class="hide" value="" id="planId-turnover" name="planConfigId"/>
            <div class="form-group">
                <label class="control-label col-sm-6" for="">名称：</label>
                <div class="col-sm-16">
                    <input type="text" class="form-control" value="" name="name">
                </div>
            </div>
            <div class="form-group">
                <label class="control-label col-sm-6" for="">运行参数：</label>

                <div class="col-sm-16">
                    <input type="text" class="form-control" value="" name="runParams">
                </div>
            </div>

            <div class="form-group">
                <label class="control-label col-sm-6" for="">日期格式：</label>

                <div class="col-sm-16">
                    <input type="text" class="form-control" value="yyyy-MM-dd" name="timeFormat">
                </div>
            </div>
            <div class="form-group">
                <label class="control-label col-sm-6 ">是否忽略错误：</label>

                <div class="col-sm-16">
                    <select name="ignoreErr" class="form-control" >
                        <option value="2" selected>否</option>
                        <option value="1">是</option>
                    </select>
                </div>
            </div>
            <div class="form-group">
                <label class="control-label col-sm-6" for="">失败重试次数：</label>

                <div class="col-sm-16">
                    <input type="text" class="form-control" value="3" name="retryCnt">
                </div>
            </div>
            <div class="form-group">
                <label class="control-label col-sm-6" for="">失败重试时间(s)：</label>

                <div class="col-sm-16">
                    <input type="text" class="form-control" value="60" name="retrySec">
                </div>
            </div>
            <input type="reset" id="form_turnover_reset" style="display:none;"/>
        </form>
    </div>

</div>

